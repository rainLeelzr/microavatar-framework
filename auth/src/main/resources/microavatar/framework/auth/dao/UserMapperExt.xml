<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="microavatar.framework.auth.dao.UserDaoExt">

    <select id="getByAccount" parameterType="string" resultType="microavatar.framework.auth.entity.User">
        SELECT
        <include refid="microavatar.framework.auth.dao.UserDao._allCloumns"/>
        from
        <include refid="microavatar.framework.auth.dao.UserDao._tableName"/>
        WHERE account = #{account}
    </select>

</mapper>